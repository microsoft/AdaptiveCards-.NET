<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net4.5.2;netstandard1.3</TargetFrameworks>
    <PackageId>Microsoft.MarkedNet</PackageId>
    <VersionPrefix>1.0.14</VersionPrefix>
    <Authors>Microsoft</Authors>
    <summary>MarkedNet Extensions for multiple ui frameworks</summary>
    <Description>Microsoft Bot Framework fork of MarkedNet library with Xaml,Text and .net standard extensions</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageTags>markdown</PackageTags>
    <PackageProjectUrl>https://github.com/Microsoft/AdaptiveCards</PackageProjectUrl>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/Microsoft/AdaptiveCards</RepositoryUrl>
    <RepositoryType></RepositoryType>
    <PackageLicenseUrl>https://github.com/Microsoft/AdaptiveCards/blob/master/LICENSE</PackageLicenseUrl>
    <AssemblyVersion>1.0.14.0</AssemblyVersion>
    <Version>1.0.14</Version>
    <FileVersion>1.0.14.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.3.*" />
    <PackageReference Include="Newtonsoft.Json" Version="[11.0.2, 12.0.3]" />
  </ItemGroup>

</Project>
